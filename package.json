{
  "name": "ngx-fomantic-ui",
  "version": "0.11.7",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lib:compile:locales": "tsc --project ./projects/ngx-fomantic-ui/src/behaviors/localization/tsconfig.locales.json && node ./projects/ngx-fomantic-ui/scripts/locales",
    "build:lib": "ng build ngx-fomantic-ui && npm run lib:compile:locales",
    "build:lib:prod": "ng build ngx-fomantic-ui --configuration production && npm run lib:compile:locales",
    "build:docs:prod": "ng build ngx-fomantic-ui-docs --configuration production",
    "serve:docs": "ng serve ngx-fomantic-ui-docs",
    "docker:serve:docs": "ng serve ngx-fomantic-ui-docs --host 0.0.0.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.6",
    "@angular/common": "^20.0.6",
    "@angular/compiler": "^20.0.6",
    "@angular/core": "^20.0.6",
    "@angular/forms": "^20.0.6",
    "@angular/platform-browser": "^20.0.6",
    "@angular/platform-browser-dynamic": "^20.0.6",
    "@angular/router": "^20.0.6",
    "@types/estree": "^1.0.5",
    "@types/ws": "^8.5.4",
    "core-js": "^3.6.5",
    "date-fns": "^1.30.1",
    "element-closest": "^3.0.2",
    "highlightjs-line-numbers.js": "^2.8.0",
    "ngx-highlightjs": "^10.0.0",
    "popper.js": "^1.16.1",
    "rxjs": "^7.8.1",
    "tslib": "^2.3.1",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.0.5",
    "@angular/cli": "^20.0.5",
    "@angular/compiler-cli": "^20.0.6",
    "@angular/language-service": "^20.0.6",
    "@types/highlight.js": "^9.12.4",
    "@types/jasmine": "~5.1.8",
    "@types/jasminewd2": "~2.0.13",
    "@types/node": "^24.0.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~5.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-packagr": "^20.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.3",
    "typescript": "^5.8.3"
  }
